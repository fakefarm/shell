alias ss='ssh triage.vm'

# bundle
alias be='bundle exec'

# helpers
alias bp='vi $HOME/shell/bash_profile' # open bash_profile
alias src='source $HOME/shell/bash_profile'
alias rs='bundle exec rspec spec'

